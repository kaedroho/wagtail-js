{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "moduleResolution": "node",
        "isolatedModules": false,
        "jsx": "react",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "filesGlob": [
        "**/*.ts",
        "**/*.tsx",
        "!node_modules/**",
        "!dist/**"
    ],
    "files": [
        "src/main.ts",
        "src/query/base.ts",
        "src/query/documents.ts",
        "src/query/images.ts",
        "src/query/pages.ts",
        "typings/es6-promise/es6-promise.d.ts",
        "typings/tsd.d.ts",
        "typings/whatwg-fetch/whatwg-fetch.d.ts"
    ],
    "exclude": []
}
